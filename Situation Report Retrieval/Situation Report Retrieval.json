                            "code": "export const code = async (inputs) => {\n  const { existingReports, newScrapedReports } = inputs;\n\n  // Flatten existing report links into a simple array of URLs\n  const existingLinks = existingReports.map(report => report.values.A);\n\n  // Filter out links from the new reports that are not in the existing links\n  const newPdfLinks = newScrapedReports.filter(url => \n    !existingLinks.includes(url) && url.endsWith('.pdf')\n  );\n\n  // If there are no new PDF links, return false\n  if (newPdfLinks.length === 0) {\n    return false;\n  }\n\n  // Return the new links found\n  return newPdfLinks;\n};",
